void displayItem4bit(const byte segment[][ARRAY_WIDTH], const byte palette[16][3], byte leng, int povrepeat) {
  
  while ( povrepeat-- > 0 ) {
    
    for (int i=0; i<leng; i++) {
      for (int j=0; j<STRIPSIZE; j+=2) {
        
        byte aVar = pgm_read_byte(&segment[i][j/2]);
        byte pIdx = (aVar & 0b11110000) >> 4;
        
        leds[j] = CRGB(palette[pIdx][0], palette[pIdx][1], palette[pIdx][2]);
      
        if (j+1<STRIPSIZE) {
          pIdx = aVar & 0b00001111;
          leds[j+1] = CRGB(palette[pIdx][0], palette[pIdx][1], palette[pIdx][2]);

        }
        
      }
      FastLED.show();
      delayMicroseconds(LINEDELAYMICRO);
      //clearStrip(); 
    }    
  }

}


void displayItem8bit(const byte segment[][STRIPSIZE], const byte palette[256][3], int leng, int povrepeat) {
  
  
  while ( povrepeat-- > 0 ) {
    
    for (int i=0; i<leng; i++) {
      for (int j=0; j<STRIPSIZE; j++) {
        
        byte pIdx = pgm_read_byte(&segment[i][j]);
        
        //leds[j] = CRGB( palette[pIdx][0], palette[pIdx][1], palette[pIdx][2]);
        leds[j] = CRGB( pgm_read_byte(&palette[pIdx][0]), pgm_read_byte(&palette[pIdx][1]), pgm_read_byte(&palette[pIdx][2]));
          
        }
        FastLED.show();
        //delayMicroseconds(LINEDELAYMICRO);
        delay(LINEDELAY);
      }
      
  }
  
}






